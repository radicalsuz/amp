    <lnav_header>
        <type>header</type>
        <default>Left Side Navigation</default>
    </lnav_header>
	<lnav3>
		<type>textarea</type>
		<label>Left Nav #1 : start Heading Row</label>
        <size>6:65</size>
	</lnav3>
	<lnav4>
		<type>textarea</type>
		<label>Left Nav #2  : end Heading Row</label>
        <size>6:65</size>
	</lnav4>
	<lnav7>
		<type>textarea</type>
		<label>Left Nav #3  : start Content Table Row : repeats</label>
        <size>6:65</size>
	</lnav7>
	<lnav8>
		<type>textarea</type>
		<label>Left Nav #4  : end Content Table Row : repeats</label>
        <size>6:65</size>
	</lnav8>
	<lnav9>
		<type>textarea</type>
		<label>Left Nav #5  : spacer</label>
        <size>6:65</size>
	</lnav9>
    <rnav_header>
        <type>header</type>
        <default>Right Side Navigation</default>
    </rnav_header>
