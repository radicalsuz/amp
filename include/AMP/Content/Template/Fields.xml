        <type>header</type>
        <default>Template Paths</default>
    </paths_header>
	<css>
		<type>text</type>
		<label>CSS Files</label>
        <default>custom/styles.css</default>
	</css>
	<imgpath>
		<type>text</type>
		<label>Image Path</label>
        <default>img/</default>
	</imgpath>
	<extra_header>
		<type>textarea</type>
		<label>Extra Headers</label>
        <size>6:65</size>
	</extra_header>

